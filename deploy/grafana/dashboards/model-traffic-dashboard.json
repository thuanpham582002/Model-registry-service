{
  "title": "Model Registry - Traffic Dashboard",
  "uid": "model-registry-traffic",
  "editable": true,
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "title": "Request Rate by Variant",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "sum by (revision_name) (rate(revision_request_count[5m]))",
          "legendFormat": "{{revision_name}}"
        }
      ],
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom" }
      }
    },
    {
      "id": 2,
      "title": "Latency P99",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum by (le, revision_name) (rate(revision_request_latencies_bucket[5m])))",
          "legendFormat": "{{revision_name}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        }
      }
    },
    {
      "id": 3,
      "title": "Latency P50",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le, revision_name) (rate(revision_request_latencies_bucket[5m])))",
          "legendFormat": "{{revision_name}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        }
      }
    },
    {
      "id": 4,
      "title": "Error Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "sum by (revision_name) (rate(revision_request_count{response_code!=\"200\"}[5m])) / sum by (revision_name) (rate(revision_request_count[5m])) * 100",
          "legendFormat": "{{revision_name}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        }
      }
    },
    {
      "id": 5,
      "title": "Traffic Split (Last 1h)",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 8, "x": 0, "y": 16 },
      "targets": [
        {
          "expr": "sum by (revision_name) (increase(revision_request_count[1h]))",
          "legendFormat": "{{revision_name}}"
        }
      ]
    },
    {
      "id": 6,
      "title": "Total Requests",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 8, "y": 16 },
      "targets": [
        {
          "expr": "sum(increase(revision_request_count[1h]))"
        }
      ],
      "options": {
        "colorMode": "value"
      }
    },
    {
      "id": 7,
      "title": "Avg Latency P99",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 12, "y": 16 },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, sum(rate(revision_request_latencies_bucket[5m])) by (le))"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        }
      }
    },
    {
      "id": 8,
      "title": "Error Rate (Overall)",
      "type": "stat",
      "gridPos": { "h": 4, "w": 4, "x": 16, "y": 16 },
      "targets": [
        {
          "expr": "sum(rate(revision_request_count{response_code!=\"200\"}[5m])) / sum(rate(revision_request_count[5m])) * 100"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "yellow", "value": 1 },
              { "color": "red", "value": 5 }
            ]
          }
        }
      }
    }
  ]
}
